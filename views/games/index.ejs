<%- include("../partials/html-head")  %> 
<%- include("../partials/nav")  %> 

<main>
  <h1>Games</h1>
  <% games.forEach(game => { %>
   <h4><%= game.awayTeam.name %> at <%= game.homeTeam.name %> </h4>
   <p> <%= game.time %></p>
  <% }) %>
  <form action="games/new" method="get">
    <button type="submit">Add Game</button>
  </form>
</main>

